# See: man pkg-config
prefix=/usr
exec_prefix=${prefix}
libdir=/usr/lib/x86_64-linux-gnu
includedir=${prefix}/include

Name: Python
Description: Embed Python into an application
Requires:
Version: 3.9
Libs.private: -lcrypt -lpthread -ldl  -lutil -lm
Libs: -L${libdir} -lpython3.9
Cflags: -I${includedir}/python3.9
